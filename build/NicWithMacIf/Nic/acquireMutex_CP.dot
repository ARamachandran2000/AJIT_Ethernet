digraph control_path {
  e_0: entry_node : n ;
  e_1 [shape = triangle];
  e_2 [shape = diamond];
  e_3 [shape = triangle];
  e_4 [shape = diamond];
  e_5 [shape = triangle];
  e_6: exit_node : s ;
  e_7 [shape = invtriangle];
  e_7 -> e_1;
  e_7 -> e_2;
  e_2 -> e_3;
  e_7 -> e_4;
  e_4 -> e_5;
  e_4 -> e_6;
  e_5 -> e_7;
  e_0 -> e_7;
}
